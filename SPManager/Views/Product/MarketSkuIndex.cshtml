
<div class="container" style="margin-left: 0px;margin-right: 0px;">
    <div class="inner clearfix">
        @Html.Partial("_ProductLeft")
        <!-- end content-left -->

        <div class="content-right ">

            <a href="javascript:;" class="btn-leftMenuFold" id="j-btn-leftMenuFold"></a>
            <h1 class="content-right-title">
                产品库存管理
                <a class="gicon-info-sign gicon_linkother" href="http://help.kuaixcx.com/Operation/index.html?id=54" target="_blank"></a>
            </h1>
            <div class="tables-searchbox newcearchbox">      
                <a href="#" class="btn btn-success j-additem" style="height: 32px;" id="btnAddButton">添加</a>
                <select id="inputSchool" class="select small newselect" style="height: 35px;">
                    <option value="" selected="">学校名称</option>
                    @foreach (SP.Application.Product.DTO.RegionDto entity in ViewBag.SchoolData as List<SP.Application.Product.DTO.RegionDto>)
                    {
                        <option value="@entity.DataId">@entity.DataName</option>
                    }
                </select>
                <select id="inputDistrict" class="select small newselect" style="height: 35px;">
                    <option value="0" selected="">院区名称</option>
                    @foreach (SP.Application.Product.DTO.RegionDto entity in ViewBag.DistrictData as List<SP.Application.Product.DTO.RegionDto>)
                    {
                        <option value="@entity.DataId">@entity.DataName</option>
                    }
                </select>
                <select id="inputShop" class="select small newselect" style="height: 35px;">
                    <option value="0" selected="">店铺名称</option>
                    @foreach (SP.Application.Shop.DTO.ShopDto entity in ViewBag.ShopData as List<SP.Application.Shop.DTO.ShopDto>)
                    {
                        <option value="@entity.Id">@entity.ShopName</option>
                    }
                </select>
                <select id="inputProduct" class="select small newselect" style="height: 35px;">
                    <option value="0" selected="">产品名称</option>
                    @foreach (SP.Application.Product.DTO.ProductsDto entity in ViewBag.ProductData as List<SP.Application.Product.DTO.ProductsDto>)
                    {
                        <option value="@entity.ProductId">@entity.ProductName</option>
                    }
                </select>
                <select id="inputSku" class="select small newselect" style="height: 35px;">
                    <option value="-1" selected="">库存状态</option>
                    <option value="1">在售</option>
                    <option value="0">售空</option>
                </select>
                <button class="btn btn-primary search_btn" id="btnSearch">查询</button>
            </div>
            <div class="tablesWrap mgt10">
                <table id="J_ItemList" class="wxtables table-order">
                    <colgroup>
                        <col width="5%">
                        <col width="10%">
                        <col width="15%">
                        <col width="15%">
                        <col width="5%">
                        <col width="5%">
                        <col width="5%">
                        <col width="5%">
                        <col width="5%">
                        <col width="10%">
                        <col width="20%">
                    </colgroup>
                    <thead>
                        <tr>
                            <td style="background:#f8f8f8"></td>
                            <td class="center" style="background:#f8f8f8">院区名称</td>
                            <td class="center" style="background:#f8f8f8">店铺名称</td>
                            <td class="center" style="background:#f8f8f8">产品名称</td>
                            <td class="center" style="background:#f8f8f8">SKU码</td>
                            <td class="center" style="background:#f8f8f8">库存</td>
                            <td class="center" style="background:#f8f8f8">订购数量</td>
                            <td class="center" style="background:#f8f8f8">警戒库存</td>
                            <td class="center" style="background:#f8f8f8">时价</td>
                            <td class="center" style="background:#f8f8f8">日期</td>
                            <td class="center" style="background:#f8f8f8">操作</td>
                        </tr>
                    </thead>

                </table>
                <div class="tables-btmctrl clearfix">

                </div>
            </div>
            <div id="pager" style="margin:40px 0"></div>
        </div>
        <!-- end content-right -->
    </div>
</div>
<script type="text/javascript">
    seajs.use(["/Scripts/marketsku/skudata"], function (Sysrole) {
        var $ = Sysrole.jQuery;
        Sysrole.SkuController.init({ el: $("#J_ItemList"), url: '' });
    });
</script>




